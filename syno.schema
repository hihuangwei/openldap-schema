objectclass ( 1.3.6.1.4.1.6574.5.2.2.1
	NAME 'synoPwdPolicy'
	SUP top
	AUXILIARY MAY ( pwdSynoExcludeNameDesc $ pwdSynoMixedCase $ pwdSynoNumber $
		pwdSynoSpecialChar $ pwdSynoExpireChange $ pwdSynoExpireMail $
		pwdSynoLoginFailInfo $ pwdSynoPwdChangeOnly $ pwdSynoPwdExpire $
		pwdSynoStrongPwd $ pwdSynoLockoutExpire $ pwdSynoMinAgeEnable $
		pwdSynoMaxAgeEnable $ pwdSynoInHistoryEnable $ pwdSynoMinLengthEnable $
		pwdSynoPwdReset $ pwdSynoExpireWarningEnable $ pwdSynoExpireWarning $
		pwdSynoExpireMailTime $ pwdSynoExpireMailBeforeDays $ pwdSynoMailTaskId $
		pwdSynoExcludeCommonPwd ) )

